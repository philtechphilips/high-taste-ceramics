{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx", "src/services/auth.service.js", "src/services/axiosInstance.js", "src/components/Header.jsx", "src/components/ReportDashboardLayout.jsx", "src/components/Sidebar.jsx", "src/app/dashboard/page.jsx", "src/app/dashboard/layout.jsx", "src/components/icons/index.jsx"],
  "exclude": ["node_modules"]
}
